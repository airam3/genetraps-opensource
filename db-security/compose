cat docker-compose-template.yml | sed 's@mongoUserTag@'"$MONGO_ADMIN_NAME"'@' | sed 's@mongoPasswordTag@'"$MONGO_ADMIN_PASSWORD"'@' > docker-compose.yml

ecs-cli compose \
--project-name genetraps-mongo \
-f docker-compose.yml \
--ecs-params ecs-params.yml service up \
--target-group-arn "arn:aws:elasticloadbalancing:"$AWS_REGION":"$AWS_ACCOUNT_ID":targetgroup/db-security-target-group/"$ECS_CLI_TG_DB_SECURITY \
--container-name db-security \
--container-port 27017 \
--aws-profile genetraps
